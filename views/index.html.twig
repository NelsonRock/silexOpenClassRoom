{% extends "layout.html.twig" %}

{% block title  %}Home{% endblock %}

{% block content %}
    {% for article in articles %}
        <article>
            <h2><a href="{{ path('article', { 'id': article.id }) }}" class="articleTitle" >{{ article.title }}</a></h2>
            <p>{{ article.content }}</p>
        </article>
    {% endfor %}
{% endblock %} 