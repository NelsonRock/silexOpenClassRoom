{% extends "layout.html.twig" %}

{% block content %}
    {% for article in articles %}
        <p>{{article.title}}</p>
    {% endfor %}

    <hr>

    {% for comment in comments %}
        <p>{{comment.id}}</p>
    {% endfor %}

    <hr>

    {% for user in users %}
        <p>{{user.username}}</p>
    {% endfor %}
{% endblock %}